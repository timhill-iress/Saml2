{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "buildsaml2",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Sustainsys.Saml2/Sustainsys.Saml2.csproj",
                "/property:GenerateFullPaths=true",
                "/property:DefineConstants=\"NETSTANDARD2_0\"",
                "/consoleloggerparameters:NoSummary",
                "--framework:netstandard2.0"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/Sustainsys.Saml2.AspNetCore2/Sustainsys.Saml2.AspNetCore2.csproj",
                "/property:GenerateFullPaths=true",
                "/property:DefineConstants=\"NETSTANDARD2_0\"",
                "/consoleloggerparameters:NoSummary",
                "--framework:netstandard2.0"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/Sustainsys.Saml2/Sustainsys.Saml2.csproj",
                "/property:GenerateFullPaths=true",
                "/property:DefineConstants=\"NETSTANDARD2_0\"",
                "/consoleloggerparameters:NoSummary",
                "--framework:netstandard2.0"
            ],
            "problemMatcher": "$msCompile"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/Sustainsys.Saml2/Sustainsys.Saml2.csproj",
                "/property:GenerateFullPaths=true",
                "/property:DefineConstants=\"NETSTANDARD2_0\"",
                "/consoleloggerparameters:NoSummary",
                "--framework:netstandard2.0"
            ],
            "problemMatcher": "$msCompile"
        }
    ]
}